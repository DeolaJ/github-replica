{
  "name": "github-replica",
  "version": "1.0.0",
  "description": "A replica of my Github repository tab",
  "main": "index.js",
  "author": "Adeola Adeyemo <adeola.adeyemoj@yahoo.com>",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url ./"
  },
  "license": "MIT",
  "dependencies": {
    "parcel-bundler": "^1.12.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "ie >= 10"
        ]
      }
    }
  }
}
